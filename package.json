{
  "name": "react-starter-kit",
  "version": "2.0.0",
  "description": "react starter kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --hot --color --node-env development --config ./scripts/webpack.dev.config.js",
    "build": "webpack --color --node-env production --config ./scripts/webpack.prod.config.js",
    "createDllDev": "webpack --color --env development --config ./scripts/webpack.dll.config.js",
    "createDllPro": "webpack --color --env production --config ./scripts/webpack.dll.config.js",
    "createDll": "npm run createDllDev && npm run createDllPro",
    "test:js": "jest --config=jest-js.config.js",
    "test:ui": "jest --config=jest-ui.config.js --runInBand",
    "huskyInstall": "node ./node_modules/husky/lib/installer/bin.js install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/One-boy/react-starter-kit.git"
  },
  "keywords": [
    "react",
    "starter",
    "kit"
  ],
  "author": "huyu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/One-boy/react-starter-kit/issues"
  },
  "homepage": "https://github.com/One-boy/react-starter-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "less": "^4.1.1",
    "less-loader": "^8.1.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.1",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss": "^8.2.10",
    "postcss-loader": "^5.2.0",
    "prettier": "2.2.1",
    "puppeteer": "^8.0.0",
    "react-refresh": "^0.10.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.32.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/runtime": "^7.13.10",
    "antd": "^4.15.1",
    "axios": "^0.21.1",
    "core-js": "^3.10.1",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,mjs,ts,tsx}": [
      "node_modules/.bin/prettier --write",
      "node_modules/.bin/eslint --fix",
      "git add"
    ],
    "src/*.{css,scss,less,json,html,md,markdown}": [
      "node_modules/.bin/prettier --write",
      "git add"
    ]
  }
}
